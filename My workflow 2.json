    {
      "parameters": {
        "jsCode": "const items = $input.all();\nconst today = new Date();\ntoday.setHours(0, 0, 0, 0);\n\nconst results = items.map(item => {\n  const tenant = item.json;\n\n  const email = tenant['Email'] || 'N/A';\n  const rentAmount = tenant['Rent Amount'] || 0;\n  const reminderSent = tenant['Reminder Sent'] || 'FALSE';\n\n  const dueDate = new Date(tenant['Due Date']);\n  dueDate.setHours(0, 0, 0, 0);\n\n  const timeDiff = dueDate.getTime() - today.getTime();\n  const daysUntil = Math.ceil(timeDiff / (1000 * 60 * 60 * 24));\n\n  tenant.daysUntil = daysUntil;\n\n  const paymentStatus = (tenant['Payment Status'] || '').toLowerCase();\n  const isUnpaid = paymentStatus === 'unpaid';\n\n  const isDueToday = daysUntil === 0;\n  const isUpcoming = daysUntil > 0 && daysUntil <= 5;\n  const isOverdue = daysUntil < 0;\n\n  let reminderType = null;\n  if (isDueToday) reminderType = 'due-today';\n  else if (isUpcoming) reminderType = 'upcoming';\n  else if (isOverdue) reminderType = 'overdue';\n\n  tenant.reminderType = reminderType;\n  tenant.daysOverdue = isOverdue ? Math.abs(daysUntil) : 0;\n\n  tenant.shouldRemind =\n    isUnpaid &&\n    reminderType &&\n    reminderSent.toUpperCase() === 'FALSE';\n\n  tenant.email = email;\n  tenant.rentAmount = rentAmount;\n\n  return { json: tenant };\n});\n\nreturn results.filter(i => i.json.shouldRemind);\n"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
        1968,
        96
      ],
      "id": "319d932c-9d61-448f-a092-66e191ebe11b",
      "name": "Filter & Categorize"
    },
    {
      "parameters": {
        "jsCode": "const items = $input.all();\n\nconst updates = items.map(item => {\n  const data = item.json;\n\n  if (!data.row_number && !data.originalData?.row_number) {\n    throw new Error('Missing row_number â€” cannot update Google Sheet row');\n  }\n\n  const rowNumber = data.row_number ?? data.originalData.row_number;\n\n  const today = new Date();\n  const dateString = `${today.getMonth() + 1}/${today.getDate()}/${today.getFullYear()}`;\n\n  return {\n    json: {\n      row_number: rowNumber,\n\n      // REQUIRED: fields Google Sheets must retain\n      'Tenant Name': data['Tenant Name'],\n      'Email': data.Email,\n      'Building': data.Building,\n      'Unit #': data['Unit #'],\n      'Rent Amount': data['Rent Amount'],\n      'Due Date': data['Due Date'],\n      'Payment Status': data['Payment Status'],\n\n      // REMINDER STATE\n      'Reminder Sent': 'TRUE',\n      'Last Reminder Type': data.reminderType,\n      'Last Reminder Date': dateString\n    }\n  };\n});\n\nreturn updates;\n"
      },
